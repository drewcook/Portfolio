    ViewBag.Title = "Work Page";
}

<!DOCTYPE html>
<html>
<head>
<link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
<link rel="icon" href="/favicon.ico" type="image/x-icon">
    <title>Work Page</title>
</head>
<body>
<div class="work-content">
<h2 align="center">Samples Of Work</h2>
<br />
<h3>Games</h3>
<div class="img" id="dragon">
<div class="image active"><img src="@Url.Content("~/Content/images/dragoncombattext.png")" alt="Dragon Combat Text" width="450" height="324"/></div>
<div class="image hide"><img src="@Url.Content("~/Content/images/dragoncombatround.png")" alt="Dragon Combat Round" width="450" height="324"/></div>
<div class="image hide"><img src="@Url.Content("~/Content/images/dragoncombatattack.png")" alt="Dragon Combat Attack" width="450" height="324"/></div>
<div class="image hide"><img src="@Url.Content("~/Content/images/dragongameend.png")" alt="Dragon Combat End" width="450" height="324"/></div>
<p><a href="@Url.Action("DragonCombat","Work")">Dragon Combat 2.0</a></p>
</div>

<div class="img" id="trivia">
    <div class="image active"><img src="@Url.Content("~/Content/images/triviagamecorrect.png")" alt="Trivia Game Correct"  width="450" height="324"/></div>
    <div class="image hide"><img src="@Url.Content("~/Content/images/triviagameincorrect.png")" alt="Trivia Game Incorrect"  width="450" height="324"/></div>
    <div class="image hide"><img src="@Url.Content("~/Content/images/triviagameresults.png")" alt="Trivia Game Results"  width="450" height="324"/></div>
    <p><a href="@Url.Action("TriviaGame","Work")">Trivia Game</a></p>
</div>

<div class="img" id="pokerplayer">
    <div class="image active"><img src="@Url.Content("~/Content/images/PokerPlayerText.png")" alt="Poker Player Text"  width="450" height="324"/></div>
    <div class="image hide"><img src="@Url.Content("~/Content/images/PokerPlayerHand.png")" alt="Poker Player Hand"  width="450" height="324"/></div>
    <div class="image hide"><img src="@Url.Content("~/Content/images/PokerPlayerHands.png")" alt="Poker Player Hands"  width="450" height="324"/></div>
<p><a href="@Url.Action("PokerPlayer", "Work")">Poker Player</a></p>
</div>

<div class="img" id="guessthatnumber">
    <div class="image active"><img src="@Url.Content("~/Content/images/guessthatnumber.png")" alt="Guess That Number Text"  width="450" height="324"/></div>
    <div class="image hide"><img src="@Url.Content("~/Content/images/guessthatnumberhighscore.png")" alt="Guess That Number End"  width="450" height="324"/></div>
    <div class="image hide"><img src="@Url.Content("~/Content/images/guessthatnumberhighscoreresult.png")" alt="Guess That Number High Scores"  width="450" height="324"/></div>
<p><a href="@Url.Action("GuessThatNumber","Work")">Guess That Number</a></p>
</div>


<h3>Music</h3>
<div class="img">
    <iframe width="250" height="180" scrolling="no" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/156962621&amp;color=ffb400&amp;auto_play=false&amp;hide_related=false&amp;show_comments=true&amp;show_user=true&amp;show_reposts=false"></iframe>
    <p>"Jerusalem Ridge"</p>
</div>

<div class="img">
   <iframe width="250" height="180" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/152506104&amp;color=ffb400&amp;auto_play=false&amp;hide_related=false&amp;show_comments=true&amp;show_user=true&amp;show_reposts=false"></iframe>
    <p>"Lonesome Fiddle Blues"</p>
</div>
    

</div>
</body>

</html>